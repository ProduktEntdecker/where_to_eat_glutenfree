# OpenStreetMap Configuration
# No API key required - OpenStreetMap is free and open source!

# Optional: Restrict search radius (in meters)
# Default: 5000 (5km)
VITE_SEARCH_RADIUS=5000

# Optional: Enable caching for API responses (recommended for production)
# Default: true
VITE_ENABLE_CACHE=true

# Optional: Cache duration in minutes
# Default: 30 (30 minutes)
VITE_CACHE_DURATION_MINUTES=30